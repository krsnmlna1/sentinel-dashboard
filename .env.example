# Sentinel Dashboard Environment Variables
# Copy this file to .env and fill in your values

# Required for blockchain data fetching
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Required for AI features (OpenAI/Groq)
GROQ_API_KEY=your_groq_api_key_here

# Optional: Node environment (Railway sets this automatically)
# NODE_ENV=production

# Optional: Port (Railway sets this automatically)
# PORT=3000
